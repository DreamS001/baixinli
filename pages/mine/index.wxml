<!--个人中心-->
<view class='top_bg'></view>
<view class='user_box'>
    <view class='user_img'>
      <image src='{{userInfo.avatarUrl}}'></image>
    </view>
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
</view>
<view class="chooseItem">
    <navigator url="./reservation/reservation" hover-class="none" class='item_box'>
      <view class='item_left'>
        <image src='../../images/mine/reservation.png'></image>
        <text>我的预约</text>
      </view>
      <image src='../../images/mine/right.png'></image>
    </navigator>
    <navigator url="./follow/follow" hover-class="none" class='item_box'>
      <view class='item_left'>
        <image src='../../images/mine/follow.png'></image>
        <text>我的关注</text>
      </view>
      <image src='../../images/mine/right.png'></image>
    </navigator>
    <navigator url="./admission/admission" hover-class="none" class='item_box'>
      <view class='item_left'>
        <image src='../../images/mine/admission.png'></image>
        <text>入驻</text>
      </view>
      <image src='../../images/mine/right.png'></image>
    </navigator>
    <navigator url="./coupon/coupon" hover-class="none" class='item_box'>
      <view class='item_left'>
        <image src='../../images/mine/coupon.png'></image>
        <text>优惠券</text>
      </view>
      <image src='../../images/mine/right.png'></image>
    </navigator>
    <navigator url="./guide/guide" hover-class="none" class='item_box'>
      <view class='item_left'>
        <image src='../../images/mine/guide.png'></image>
        <text>咨询指南</text>
      </view>
      <image src='../../images/mine/right.png'></image>
    </navigator>
  </view>