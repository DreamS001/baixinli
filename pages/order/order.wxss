.coupon{
  width: 100%;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.coupon .coupon_item{
  width: 100%;
  height: 180rpx;
}

.couponBox{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.coupon_items{
  width: 100%;
  height: 180rpx;
  margin-top: 20rpx;
  display: flex;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #6dcaaa;
  background:url("https://www.baixl001.com/mentality/picture/010.png");
  background-size:cover;
}
.coupon_items .coupon_price{
  width: 30%;
  font-size: 37rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #6dcaaa;
  margin-right: 33rpx;
}
.coupon_items .coupon_price text{
  font-size: 72rpx;
	font-weight: normal;
	letter-spacing: 0rpx;
}
.coupon_items .coupon_desc{
  width: 55%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #333333;
  margin-right: 37rpx;
  margin-top: 20rpx;
}
.coupon_items .coupon_desc text{
  line-height: 40rpx;
}
.coupon_items .coupon_desc text:last-child{
  font-size: 19rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #999999;
}
.coupon_items .coupon_stats{
  width: 15%;
	height: 100rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.warrinfo{
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #6dcaaa;
}